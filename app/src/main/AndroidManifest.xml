<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xie.designpatterns">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".MAplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".bezier.BezierActivity" />
        <activity android:name=".wave.WaveActivity" />
        <activity android:name=".widget.BounchingActivity" />
        <activity android:name=".recyckerview.itemtouchhelper.ItemTouchActivity" />
        <activity android:name=".app.ExceptionActivity" />
        <activity android:name=".loadtaost.LoadToastActivity" />
        <activity android:name=".parallax.SplashActivity" />
        <activity android:name=".pricetag.PriceTagActivity" />

        <receiver android:name=".app.StartAppReceiver" />

        <activity android:name=".palette.PaletteActivity" />
        <activity android:name=".translucentscrollivew.TranslucentActivity" />
        <activity android:name=".snakerbar.SnakeBarActivity" />
        <activity android:name=".fb.FabActivity" />
        <activity android:name=".cardview.CardViewActivity" />
        <activity android:name=".fb.BehaviorActivity" />
        <activity android:name=".parallel.ParalleActivity" />
        <activity android:name=".behavior.CustomeBehaviorActivity" />
        <activity android:name=".buffer.BufferActivity" />
        <activity android:name=".chinamap.ChinaMapActivity" />
        <activity android:name=".shader.MyGradientActivity"/>
        <activity android:name=".reveal.RevealActivity"/>
        <activity android:name=".searchview.MySearchViewActivity"/>
        <activity android:name=".design.besiz.WaveActivity"/>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />
    </application>
</manifest>